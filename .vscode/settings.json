{
	"editor.defaultFormatter": "biomejs.biome",
	"editor.formatOnSave": false,
	"editor.formatOnPaste": true,
	"emmet.showExpandedAbbreviation": "never",
	"tailwindCSS.experimental.classRegex": [
		["cn\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"]
	],
	"editor.codeActionsOnSave": {
		"quickfix.biome": "explicit",
		"source.organizeImports.biome": "explicit"
	},
	"typescript.preferences.importModuleSpecifier": "non-relative",
	"typescript.tsdk": "node_modules/typescript/lib",
	"i18n-ally.localesPaths": ["src/lib/i18n/translations"],
	"i18n-ally.keystyle": "nested",
	"i18n-ally.enabledFrameworks": ["next-intl"],
	"i18n-ally.keysInUse": ["mail.organizationInvitation.headline"],
	"i18n-ally.tabStyle": "tab",
	"[typescript]": {
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[prisma]": {
		"editor.defaultFormatter": "Prisma.prisma"
	},
	"workbench.colorCustomizations": {
		"activityBar.background": "#052E55",
		"titleBar.activeBackground": "#074178",
		"titleBar.activeForeground": "#F9FCFF"
	},
	"cSpell.words": ["hackathon", "Wechat"],
	"WillLuke.nextjs.addTypesOnSave": true,
	"WillLuke.nextjs.hasPrompted": true
}
