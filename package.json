{
	"name": "community",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"build": "next build --webpack --experimental-build-mode=compile",
		"buildskiptype": "SKIP_TYPE_CHECK=true next build",
		"dev": "next dev --turbopack",
		"start": "next start",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --fix",
		"format": "biome format . --write --no-errors-on-unmatched",
		"lint:fast": "oxlint --tsconfig tsconfig.json --quiet src",
		"lint:all": "biome format . --write --no-errors-on-unmatched && biome lint . --fix && oxlint --tsconfig tsconfig.json --quiet src",
		"type-check": "tsc --noEmit",
		"type-check:components": "tsc --noEmit --project tsconfig.components.json",
		"type-check:modules": "tsc --noEmit --project tsconfig.modules.json",
		"type-check:watch": "tsc --noEmit --watch --preserveWatchOutput",
		"check-config": "node scripts/check-config.js",
		"lint:unused": "knip",
		"lint:unused:production": "knip --production",
		"e2e": "playwright test --ui",
		"e2e:ci": "playwright install && playwright test",
		"i18n:analyze": "node scripts/i18n-full-analysis.js",
		"i18n:check": "i18n-check --locales src/lib/i18n/translations --source en --format next-intl",
		"shadcn-ui": "npx shadcn@latest",
		"postinstall": "prisma generate --no-hints --schema=./src/lib/database/prisma/schema.prisma",
		"db:generate": "prisma generate --no-hints --schema=./src/lib/database/prisma/schema.prisma",
		"db:push": "dotenv -c -- prisma db push --skip-generate --schema=./src/lib/database/prisma/schema.prisma",
		"db:migrate": "dotenv -c -- prisma migrate dev --schema=./src/lib/database/prisma/schema.prisma",
		"db:studio": "dotenv -c -- prisma studio --schema=./src/lib/database/prisma/schema.prisma",
		"db:seed": "dotenv -c -- tsx src/lib/database/seed.ts",
		"db:seed-volunteer-roles": "dotenv -c -- node scripts/seed-volunteer-roles.js",
		"content-moderation-test": "dotenv -c -- tsx scripts/content-moderation-test.ts",
		"prepare": "husky"
	},
